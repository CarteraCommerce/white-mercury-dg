<datafile-definitions>
  <File-output-definition delimiter="," filename="t_master_100.json" name="generate_tmaster_json_100" numOfRecs="100">
    <description>
    	/* 
		 * T_MASTER Field Map for generation
		 * 
		 *  CID							INT				UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,  **Generated by INSERT statement automatically based on schema
		 *  EMAIL						VARCHAR(128)	NOT NULL,                                      **Generated from white-mercury-dg dictionaries
		 *  SEED_ID						INT				UNSIGNED NULL,                                 **Only used for proofing; can be ignored for campaign building; NULL for all production data
		 *  UNSUB_FLAG					VARCHAR(1)		NOT NULL DEFAULT 'N',                          **Either 'Y' or 'N'; a healthy client would have around 30% 'Y' in their master table
		 *  UNDEL_FLAG					VARCHAR(1)		NOT NULL DEFAULT 'N',                          **Either 'Y' or 'N'; a healthy client would have around 20% 'Y' in their master table
		 *  PREFERED_MIME				VARCHAR(1)		NULL,                                          **Either NULL, 'H', 'M', or 'T'; a healthy client would have a 95/3/1/1 in their master table
		 *  ADD_DATE					TIMESTAMP		DEFAULT NOW(),                                 **Generated by INSERT statement automatically based on schema
		 *  ADD_USER_ID					VARCHAR(20)		NULL,                                          **Default to user ID of person running the white-mercury-dg application
		 *  UPDATE_DATE					TIMESTAMP		NULL,                                          **Generated as NULL
		 *  UPDATE_USER_ID				VARCHAR(20)		NULL,                                          **Generated as NULL
		 *  SOURCE						VARCHAR(80)		NOT NULL DEFAULT 'SQL INSERT',                 **Generated from dictionary containing filenames and app names; Original source of the record
		 *  MAILACTION					VARCHAR(1)		NOT NULL DEFAULT '1',                          **Values '1' through '6'; Not used during audience building/Field used in legacy proofing
		 *  FIRSTNAME					VARCHAR(50)		NULL,                                          **Generated from white-mercury-dg dictionaries
		 *  LASTNAME					VARCHAR(50)		NULL,                                          **Generated from white-mercury-dg dictionaries
		 *  FULLNAME					VARCHAR(100)	NULL,                                          **Generated as NULL.  Ignored in a healthy client
		 *  HTML_DETECTED				VARCHAR(1)		NULL,                                          **Either NULL or 'Y'; a healthy client would have around 50% 'Y' in their master table
		 *  FLASH_DETECTED				VARCHAR(1)		NULL,                                          **NULL in a healthy client
		 *  USER_AGENT					VARCHAR(128)	NULL,                                          **NULL in a healthy client
		 *  UNDEL_COUNT					NUMERIC			NULL,                                          **Generated value of null.  Value 1 to 250 with a logrithmic decrease approaching 250 in a healthy client
		 *  LAST_UNDEL_CELL				NUMERIC			NULL,                                          **NULL in a healthy client
		 *  UNDEL_FLAG_DATE				TIMESTAMP		NULL,                                          **Date that the UNDEL_FLAG was set.  Randomly dispursed.
		 *  UNSUB_FLAG_DATE				TIMESTAMP		NULL,                                          **Date that the UNSUB_FLAG was set.  Randomly dispursed.
		 *  SEED_GROUP					NUMERIC			NULL,                                          **NULL in a healthy client
		 *  UNDEL_REASON				NUMERIC			NULL,                                          **NULL if UNDEL_FLAG is not set. FK in another table. Valid values are between 1 and 12000. Generated to NULL.
		 *  MOBILE_OPTIN				VARCHAR(1)		NULL,                                          **NULL, 'Y', or 'N'. 1% 'Y' and 4% 'N' in a healthy client
		 *  MOBILE_OPTIN_CHANGE_DATE	TIMESTAMP		NULL,                                          **Set when OPTIN is set.  Only past dates randomly dispursed are valid.
		 *  MOBILE_OPTIN_SOURCE			VARCHAR(30)		NULL,                                          **NULL or 'ENROLLAPP'. 'ENROLLAPP' is 1% in a healthy client.
		 *  GENDER						VARCHAR(1)		NULL,                                          **NULL, 'M', or 'F'.  Mostly NULL in a healthy client with 24% 'M' and 26% 'F'
		 *  INCOME						NUMERIC			NULL                                           **NULL or even thousands between 70k and 290k.  Mostly NULL in a healthy client with 5% reporting
		 *  BIRTH_DATE					DATE			NULL                                           **NULL or random dates between 1960 and 2000.  Mostly NULL in a healthy client with 5% reporting
		 */
    </description>
	<data-item name="CID" type="number" alignment="1" encloseChar="" randomiser-instance="SimpleSequencer" width="-1"/>
	<data-item name="EMAIL" type="string" alignment="1" encloseChar="'" randomiser-instance="EmailSynch" width="-1"/>
	<data-item name="SEED_ID" type="number" alignment="1" encloseChar="'" randomiser-instance="NullLongRandomiser" width="-1"/>
	<data-item name="UNSUB_FLAG" type="string" alignment="1" encloseChar="" randomiser-instance="SimpleYNNoNulls30Y" width="-1"/>
	<data-item name="UNDEL_FLAG" type="string" alignment="1" encloseChar="" randomiser-instance="SimpleYNNoNulls20Y" width="-1"/>
	<data-item name="PREFERED_MIME" type="string" alignment="1" encloseChar="" randomiser-instance="SimpleHMTList" width="-1"/>
	<data-item name="ADD_DATE" type="string" alignment="1" encloseChar="" randomiser-instance="DateOn20130719" width="-1"/>
	<data-item name="ADD_USER_ID" type="string" alignment="1" encloseChar="" randomiser-instance="FixedUserID" width="-1"/>
	<data-item name="UPDATE_DATE" type="string" alignment="1" encloseChar="" randomiser-instance="NullDateRandomiser" width="-1"/>
	<data-item name="UPDATE_USER_ID" type="string" alignment="1" encloseChar="" randomiser-instance="NullStringRandomiser" width="-1"/>
	<data-item name="SOURCE" type="string" alignment="1" encloseChar="" randomiser-instance="FixedWhiteMercury-DGSource" width="-1"/>
	<data-item name="MAILACTION" type="string" alignment="1" encloseChar="" randomiser-instance="SimpleMailActionList" width="-1"/>
    <data-item name="FIRSTNAME" type="string" alignment="1" encloseChar="'" randomiser-instance="EnglishFirstnames" width="-1"/>
    <data-item name="LASTNAME" type="string" alignment="1" encloseChar="'" randomiser-instance="EnglishLastnames" width="-1"/>
    <data-item name="FULLNAME" type="string" alignment="1" encloseChar="'" randomiser-instance="NullStringRandomiser" width="-1"/>
    <data-item name="HTML_DETECTED" type="string" alignment="1" encloseChar="'" randomiser-instance="SimpleYNullRandomiser50Y" width="-1"/>
    <data-item name="FLASH_DETECTED" type="string" alignment="1" encloseChar="'" randomiser-instance="NullStringRandomiser" width="-1"/>
    <data-item name="USER_AGENT" type="string" alignment="1" encloseChar="'" randomiser-instance="NullStringRandomiser" width="-1"/>
    <data-item name="UNDEL_COUNT" type="number" alignment="1" encloseChar="'" randomiser-instance="NullLongRandomiser" width="-1"/>
    <data-item name="LAST_UNDEL_CELL" type="number" alignment="1" encloseChar="'" randomiser-instance="NullLongRandomiser" width="-1"/>
    <data-item name="UNDEL_FLAG_DATE" type="string" alignment="1" encloseChar="'" randomiser-instance="YTDTimestampRandomiser" width="-1"/>
    <data-item name="UNSUB_FLAG_DATE" type="string" alignment="1" encloseChar="'" randomiser-instance="YTDTimestampRandomiser" width="-1"/>
    <data-item name="SEED_GROUP" type="number" alignment="1" encloseChar="'" randomiser-instance="NullLongRandomiser" width="-1"/>
    <data-item name="UNDEL_REASON" type="number" alignment="1" encloseChar="'" randomiser-instance="NullLongRandomiser" width="-1"/>
    <data-item name="MOBILE_OPTIN" type="string" alignment="1" encloseChar="'" randomiser-instance="SimpleYNNullRandomiser1Y4N95Null" width="-1"/>
    <data-item name="MOBILE_OPTIN_CHANGE_DATE" type="string" alignment="1" encloseChar="'" randomiser-instance="YTDTimestampRandomiser" width="-1"/>
    <data-item name="MOBILE_OPTIN_SOURCE" type="string" alignment="1" encloseChar="'" randomiser-instance="FixedEnrollAppSource" width="-1"/>
    <data-item name="GENDER" type="string" alignment="1" encloseChar="" randomiser-instance="SynchGenderWith50Null" width="-1"/>
    <data-item name="INCOME" type="number" alignment="1" encloseChar="" randomiser-instance="IncomeRandomiserWith95Null" width="-1"/>
    <data-item name="BIRTH_DATE" type="string" alignment="1" encloseChar="" randomiser-instance="DatesFrom1950To2000with95Null" width="-1"/>
  </File-output-definition>
